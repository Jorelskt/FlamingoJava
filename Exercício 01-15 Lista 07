package exercicio07;

import java.util.Scanner;

public class Exercicio06 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Informe o nome do primeiro time: ");
		String nome1 = sc.nextLine();
		System.out.println("Informe o nome do segundo time: ");
		String nome2 = sc.nextLine();
		
		System.out.println("Informe o numero de gols do "+nome1);
		int gol1 = sc.nextInt();
		System.out.println("Informe o numero de gols do "+nome2);
		int gol2 = sc.nextInt();
		
		if (gol1 == gol2) {
			System.out.println("Empate");
		} else if (gol1 > gol2) {
			System.out.println("O vencedor é o "+nome1);
		} else {
			System.out.println("O vencedor é o "+nome2);
		}
sc.close();
	}
}
