package exercicio07;

import java.util.Scanner;

public class Exercicio10 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Informe a idade do primeiro homem: ");
		int valor1 = sc.nextInt();
		System.out.println("Informe a idade do segundo homem: ");
		int valor2 = sc.nextInt();
		System.out.println("Informe a idade da primeira mulher: ");
		int valor3 = sc.nextInt();
		System.out.println("Informe a idade da segunda mulher: ");
		int valor4 = sc.nextInt();
		
		if ((valor1 == valor2) || (valor3 == valor4)) {
			System.out.println("Inválido");
			
		} else if ((valor1 > valor2) && (valor3 > valor4)){
			System.out.println(valor1 + valor4);
			System.out.println(valor2 + valor3);
		
		} else if ((valor2 > valor1) && (valor4 > valor3)) {
			System.out.println(valor2 + valor3);
			System.out.println(valor1 + valor4);
		
		} else if ((valor3 > valor4) && (valor1 > valor2)) {
			System.out.println(valor3 + valor2);
			System.out.println(valor4 + valor1);
			
		}else if ((valor4 > valor3) && (valor2 > valor1)) {
			System.out.println(valor4 + valor1);
			System.out.println(valor3 + valor2);
		}
sc.close();
	}
}
