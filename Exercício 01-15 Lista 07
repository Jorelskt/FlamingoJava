9)
package exercicio07;

import java.util.Scanner;

public class Exercicio09 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		double valorAlcool = 2.90;
		double valorGasolina = 3.30;

		System.out.println("Informe o numero de litros vendido: ");
		int litroV = sc.nextInt();
		System.out.println("Qual o tipo do combustivel é 1 - Álcool ou 2 - Gasolina? ");
		int tipoC = sc.nextInt();
		
		double tenta = valorAlcool * litroV;
		double tenta2 = valorGasolina * litroV;
		
		double sominhaA = valorAlcool * 3 / 100 * valorAlcool;
		double sominhaA2 = valorAlcool * 5 / 100 * valorAlcool;
		double sominhaG = valorGasolina * 4 / 100 * valorGasolina;
		double sominhaG2 = valorGasolina * 6 / 100 * valorGasolina;
		
		
		double preco1 = tenta - sominhaA;
		double preco2 = tenta2 - sominhaA2;
		double preco3 = tenta - sominhaG;
		double preco4 = tenta2 - sominhaG2;
		
		if ((tipoC == 1) && (litroV <= 20)) {
			System.out.println("Quantidade de litros de Álcool vendido é de: " + preco1);
			
		} else if ((tipoC == 1) && (litroV > 20)) {
			System.out.println("Quantidade de litros de Álcool vendido é de: " + preco2);
			
		} else if ((tipoC == 2) && (litroV <= 20)) {
			System.out.println("Quantidade de litros de Gasolina vendida é de: " + preco3);
			
		} else if ((tipoC == 2) && (litroV > 20)) {
			System.out.println("Quantidade de litros de Gasolina vendida é de: " + preco4);
		}
sc.close();	
	}
}
