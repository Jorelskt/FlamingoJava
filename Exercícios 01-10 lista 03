package exercicios03;

import java.util.Scanner;

public class Exercicio04 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Digite o valor de fabricação do carro: ");
		float valorCarro = sc.nextFloat();
		float valorTotalCarro=valorCarro*28/100+valorCarro*45/100+valorCarro;
		System.out.println("O valor total do carro com imposto mais distribuidor é: "+valorTotalCarro);
	
	sc.close();
	}

}
