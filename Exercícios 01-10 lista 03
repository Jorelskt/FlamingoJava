6)
package exercicios03;

import java.util.Scanner;

public class Exercicio06 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Digite a temperatura em Fahrenheit que deseja ver em Celsios");
		double grauFahrenheit = sc.nextDouble();
		
		double grauCelsius=(grauFahrenheit-32)*5/9;
		System.out.println("A temperatura em Celsius é: "+grauCelsius);
	
		sc.close();
	}

}
