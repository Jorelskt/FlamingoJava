package exercicios03;

import java.util.Scanner;

public class Exercicio03 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		System.out.println("Digite o valor do salario mensal do funcionario: ");
		float valorSalarioMensal = sc.nextFloat();
		System.out.println("Digite o percentual do reajuste: ");
		float percentualReajuste = sc.nextFloat();
		float novoSalario=valorSalarioMensal/100*percentualReajuste+valorSalarioMensal;
		System.out.println("O valor do novo sálario é: "+novoSalario);
		
	sc.close();
	}

}
