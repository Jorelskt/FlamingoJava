3)
package exercicio06;

import java.util.Scanner;

public class Exercicio03 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner (System.in);
		
		System.out.println("Informe a primeira nota: ");
		int primeiraNota = sc.nextInt();
		System.out.println("Informe a segunda nota: ");
		int segundaNota = sc.nextInt();
		System.out.println("Informe a terceira nota: ");
		int terceiraNota= sc.nextInt();
		System.out.println("Informe a quarta nota: ");
		int quartaNota = sc.nextInt();
		int resultadoFinal=(primeiraNota+segundaNota+terceiraNota+quartaNota)/4;
		if (resultadoFinal>=5) {
			System.out.println("Parabens você está aprovado! :D");
		} else {
			System.out.println("Desculpe você está reprovado! :(");
		}
sc.close();
	}

}
